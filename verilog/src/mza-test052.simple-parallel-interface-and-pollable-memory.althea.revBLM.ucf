# written 2021-10-13 by mza
# based on mza-test051.simple-parallel-interface-and-pollable-memory.6bit-oserdes-R2R-ladder-DAC.althea.revBLM.v
# for an althea revBLM
# last updated 2021-10-21 by mza

# 100 MHz local oscillator:
net clock100_p loc=p127 | iostandard=lvds_33 | diff_term=true; # CLOCK100+
net clock100_n loc=p126 | iostandard=lvds_33 | diff_term=true; # CLOCK100-
net "clock100_p" tnm_net = tnm_clock100_p;
timespec "ts_clock100_p" = period "tnm_clock100_p" 10.0 ns high 50%; # 100 MHz

#net "_p" tnm_net = tnm_remote_clock_p;
#timespec "ts_remote_clock_p" = period "tnm_remote_clock_p" 1.965 ns high 50%; # 508.9 MHz
#net "_p" tnm_net = tnm_local_clock_p;
#timespec "ts_local_clock_p" = period "tnm_local_clock_p" 1.965 ns high 50%; # 508.9 MHz

#net "_p" tnm_net = ftsw_p;
#timespec "ts_ftsw_p" = period "ftsw_p" 7.8 ns high 50%; # 128.205 MHz

# first column, closest to FPGA on left side of board
net a_n loc=p116 | iostandard=lvcmos33 | drive = 4 | slew = slow; # A- bank0 TR
net a_p loc=p117 | iostandard=lvcmos33 | drive = 4 | slew = slow; # A+ bank0 TR
net c_n loc=p131 | iostandard=lvcmos33 | drive = 4 | slew = slow; # C- bank0 TR
net c_p loc=p132 | iostandard=lvcmos33 | drive = 4 | slew = slow; # C+ bank0 TR
net d_n loc=p139 | iostandard=lvcmos33 | drive = 4 | slew = slow; # D- bank0 TR
net d_p loc=p140 | iostandard=lvcmos33 | drive = 4 | slew = slow; # D+ bank0 TR
net f_n loc=p141 | iostandard=lvcmos33 | drive = 4 | slew = slow; # F- bank0 TL
net f_p loc=p142 | iostandard=lvcmos33 | drive = 4 | slew = slow; # F+ bank0 TL

# middle column, on left side of board
net b_n loc=p123 | iostandard=lvcmos33 | drive = 4 | slew = slow; # B- bank0 TR
net b_p loc=p124 | iostandard=lvcmos33 | drive = 4 | slew = slow; # B+ bank0 TR
net e_n loc=p133 | iostandard=lvcmos33 | drive = 4 | slew = slow; # E- bank0 TL
net e_p loc=p134 | iostandard=lvcmos33 | drive = 4 | slew = slow; # E+ bank0 TL

# third column, on left side of board
net z loc=p17  | iostandard=lvcmos33 | drive = 4 | slew = slow; # Z bank3 TL - conflicts with led<4> when using cascaded serdes
net y loc=p15  | iostandard=lvcmos33 | drive = 4 | slew = slow; # Y bank3 TL - conflicts with led<5> when using cascaded serdes
net x loc=p12  | iostandard=lvcmos33 | drive = 4 | slew = slow; # X bank3 TL - conflicts with led<6> when using cascaded serdes
net w loc=p10  | iostandard=lvcmos33 | drive = 4 | slew = slow; # W bank3 TL - conflicts with led<7> when using cascaded serdes
net v loc=p8   | iostandard=lvcmos33 | drive = 4 | slew = slow; # V bank3 TL - conflicts with rot<1> when using cascaded serdes
net u loc=p2   | iostandard=lvcmos33 | drive = 4 | slew = slow; # U bank3 TL - conflicts with button when using cascaded serdes

# first column, closest to FPGA on right side of board
net m_p loc=p58  | iostandard=lvcmos33 | drive = 4 | slew = slow; # M+ bank2 BR
net m_n loc=p57  | iostandard=lvcmos33 | drive = 4 | slew = slow; # M- bank2 BR
net l_p loc=p48  | iostandard=lvcmos33 | drive = 4 | slew = slow; # L+ bank2 BR
net l_n loc=p47  | iostandard=lvcmos33 | drive = 4 | slew = slow; # L- bank2 BR
net j_p loc=p46  | iostandard=lvcmos33 | drive = 4 | slew = slow; # J+ bank2 BL
net j_n loc=p45  | iostandard=lvcmos33 | drive = 4 | slew = slow; # J- bank2 BL
net g_p loc=p41  | iostandard=lvcmos33 | drive = 4 | slew = slow; # G+ bank2 BL
net g_n loc=p40  | iostandard=lvcmos33 | drive = 4 | slew = slow; # G- bank2 BL

# middle column, on right side of board
net k_p loc=p56  | iostandard=lvcmos33 | drive = 4 | slew = slow; # K+ bank2 BR
net k_n loc=p55  | iostandard=lvcmos33 | drive = 4 | slew = slow; # K- bank2 BR
net h_p loc=p44  | iostandard=lvcmos33 | drive = 4 | slew = slow; # H+ bank2 BL
net h_n loc=p43  | iostandard=lvcmos33 | drive = 4 | slew = slow; # H- bank2 BL

# third column, on right side of board
net n loc=p30  | iostandard=lvcmos33 | drive = 4 | slew = slow; # N bank3 TL
net p loc=p35  | iostandard=lvcmos33 | drive = 4 | slew = slow; # P bank3 TL - conflicts with rot<4> when using cascaded serdes
net q loc=p22  | iostandard=lvcmos33 | drive = 4 | slew = slow; # Q bank3 TL - differs from althea revB
net r loc=p27  | iostandard=lvcmos33 | drive = 4 | slew = slow; # R bank3 TL
net s loc=p24  | iostandard=lvcmos33 | drive = 4 | slew = slow; # S bank3 TL
net t loc=p51  | iostandard=lvcmos33 | drive = 4 | slew = slow; # T bank2 ?? - differs from althea revB and revBL - conflicts with rot<2> when using cascaded serdes

# raspberry pi 40pin headers, general purpose IO:
net rpi_gpio2_i2c1_sda  loc=p138 | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank0 broadcom i2c peripheral
net rpi_gpio3_i2c1_scl  loc=p137 | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank0 broadcom i2c peripheral
net rpi_gpio4_gpclk0    loc=p88  | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank1 broadcom gpclk peripheral - differs from althea revB
net rpi_gpio5           loc=p66  | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank2
net rpi_gpio6_gpclk2    loc=p67  | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank2 broadcom gpclk peripheral - differs from althea revB and revBL
net rpi_gpio7_spi_ce1   loc=p80  | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank1 broadcom spi peripheral
net rpi_gpio8_spi_ce0   loc=p82  | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank1 broadcom spi peripheral
net rpi_gpio9_spi_miso  loc=p112 | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank0 broadcom spi peripheral
net rpi_gpio10_spi_mosi loc=p111 | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank0 broadcom spi peripheral
net rpi_gpio11_spi_sclk loc=p93  | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank1 broadcom spi peripheral
net rpi_gpio12          loc=p78  | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank1
net rpi_gpio13          loc=p61  | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank2
net rpi_gpio14          loc=p99  | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank1 - differs from althea revB
net rpi_gpio15          loc=p118 | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank0
net rpi_gpio16          loc=p59  | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank2
net rpi_gpio17          loc=p101 | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank1
net rpi_gpio18          loc=p115 | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank0
net rpi_gpio19          loc=p87  | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank1 - differs from althea revB
net rpi_gpio20          loc=p83  | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank1
net rpi_gpio21          loc=p79  | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank1 - differs from althea revB
net rpi_gpio22          loc=p62  | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank - differs from althea revB and revBL
net rpi_gpio23          loc=p100 | iostandard=lvcmos33 | drive = 4 | slew = slow; # bank - differs from althea revB and revBL
#net rpi_gpio24                                                                 ; # used for jtag (TDO)
#net rpi_gpio25                                                                 ; # used for jtag (TCK) - differs from althea revB and revBL
#net rpi_gpio26                                                                 ; # used for jtag (TMS) - differs from althea revB and revBL
#net rpi_gpio27                                                                 ; # used for jtag (TDI)

#net "rpi_gpio4_gpclk0" tnm_net = tnm_clock10;
#timespec "ts_clock10" = period "tnm_clock10" 100.0 ns high 50%; # 10 MHz

# hexadecimal rotary encoder:
net rot<0> loc=p7  | iostandard=lvcmos33 | pullup; # rot_1 bank0 - differs from althea revB and revBL - conflicts with v when using cascaded serdes
net rot<1> loc=p50 | iostandard=lvcmos33 | pullup; # rot_2 bank0 - differs from althea revB and revBL - conflicts with t when using cascaded serdes
net rot<2> loc=p34 | iostandard=lvcmos33 | pullup; # rot_4 bank0 - differs from althea revB and revBL - conflicts with p when using cascaded serdes
#net rot<3> loc=p97 | iostandard=lvcmos33 | pullup; # rot_8 bank0 - differs from althea revB and revBL

# momentary pushbutton, normally open:
net button loc=p1 | iostandard=lvcmos33 | pullup; # bank3 - on same p/n pair as U

# array of 8 LEDs:
# available when LEFT_DAC mode is used:
#net led<0> loc=p29 | iostandard=lvcmos33 | drive = 2 | slew = slow; # on same p/n pair as N
#net led<1> loc=p26 | iostandard=lvcmos33 | drive = 2 | slew = slow; # on same p/n pair as R
#net led<2> loc=p23 | iostandard=lvcmos33 | drive = 2 | slew = slow; # on same p/n pair as S
#net led<3> loc=p21 | iostandard=lvcmos33 | drive = 2 | slew = slow; # on same p/n pair as Q
# available when RIGHT_DAC mode is used:
#net led<4> loc=p16 | iostandard=lvcmos33 | drive = 2 | slew = slow; # on same p/n pair as Z
#net led<5> loc=p14 | iostandard=lvcmos33 | drive = 2 | slew = slow; # on same p/n pair as Y
#net led<6> loc=p11 | iostandard=lvcmos33 | drive = 2 | slew = slow; # on same p/n pair as X
#net led<7> loc=p9  | iostandard=lvcmos33 | drive = 2 | slew = slow; # on same p/n pair as W

# 4 more LEDs, one next to each of the first 4 coax connectors:
net coax_led<0> loc=p102 | iostandard=lvcmos33 | drive = 2 | slew = slow; # differs from althea revB
net coax_led<1> loc=p98  | iostandard=lvcmos33 | drive = 2 | slew = slow; # differs from althea revB
net coax_led<2> loc=p92  | iostandard=lvcmos33 | drive = 2 | slew = slow;
net coax_led<3> loc=p81  | iostandard=lvcmos33 | drive = 2 | slew = slow; # differs from althea revB

net coax<0> loc=p105 | iostandard=lvcmos33 | drive = 24 | slew = fast; # bank1
net coax<1> loc=p95  | iostandard=lvcmos33 | drive = 24 | slew = fast; # bank1 - differs from althea revB
net coax<2> loc=p85  | iostandard=lvcmos33 | drive = 24 | slew = fast; # bank1 - differs from althea revB
net coax<3> loc=p75  | iostandard=lvcmos33 | drive = 24 | slew = fast; # bank1
net coax<4> loc=p6   | iostandard=lvcmos33 | drive = 24 | slew = fast; # bank3
net coax<5> loc=p33  | iostandard=lvcmos33 | drive = 24 | slew = fast; # bank3 - differs from althea revB

net "button" tig;
net "rot<?>" tig;
net "althea/word_clock_sel<?>" tig;
#net "led<?>" tig;
net "coax_led<?>" tig;
net "*cdc*" tig;

config vccaux=3.3;

