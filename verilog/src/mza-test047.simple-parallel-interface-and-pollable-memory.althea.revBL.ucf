# third column, on right side of board
net n loc=p30  | iostandard=lvcmos33; # N bank3 TL
net p loc=p35  | iostandard=lvcmos33; # P bank3 TL
net q loc=p22  | iostandard=lvcmos33; # Q bank3 TL - differs from althea revB
